<div class="container">
  <h1>All events:</h1>
  <% @events.each do |event| %>
    <h3><%= event.name %></h3>
    <p>Description:</p>
    <p><%= event.description %></p>
    <%= link_to "More details", court_event_path(event.court, event), class: "btn btn-primary"%>
    <% event.event_users.each do |event_user| %>
      <%= link_to profile_path(username: event_user.user.username) do %>
        <p><%= event_user.user.username %></p>
      <% end %>
    <% end %>
  <% end %>
</div>
